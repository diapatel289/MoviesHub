                      "type": "integer",
                      "format": "int32"
                    }
                  },
                  "CategoryIds": {
                    "type": "array",
                    "items": {
                      "type": "integer",
                      "format": "int32"
                    }
                  },
                  "QualityIds": {
                    "type": "array",
