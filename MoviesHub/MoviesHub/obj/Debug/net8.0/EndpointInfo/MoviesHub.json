                    }
                  },
                  "CastCrews": {
                    "type": "string"
                  },
                  "CastCrewList": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CastCrewDto"
                    }
                  },
                  "Screenshots": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              },
              "encoding": {
                "Name": {
                  "style": "form"
                },
                "Image": {
                  "style": "form"
                },
